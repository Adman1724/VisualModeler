    var zoom = 3;
    function zoomIn() {
        if (zoom < 7) zoom++;
        var zoombox = [0.25, 0.5, 0.75,1,1.25,1.5,1.75,2]
      
        canvas.setZoom(zoombox[zoom]);
    }
    function zoomOut() {
